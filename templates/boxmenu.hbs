<div class="menu-container" role="region" aria-label="{{_globals._menu._boxmenu.ariaRegion}}">
	<div class='menu-container-inner box-menu-inner clearfix'>
		<a href="#" class="menu-item-control menu-item-control-left icon-controls-left icon menu-item-control-hide" role="button" aria-label=""></a>
		<a href="#" class="menu-item-control menu-item-control-right icon-controls-right icon menu-item-control-hide" role="button" aria-label=""></a>
		<a href="#" class="menu-item-control menu-home-control-left icon-controls-left icon menu-item-control-hide" role="button" aria-label=""></a>
		<a href="#" class="menu-item-control menu-home-control-right icon-controls-right icon menu-item-control-hide" role="button" aria-label=""></a>
		<button class="carouselpushLeft"></button>
		<button class="carouselpushRight"></button>
		<div class="menu-header" style="background: url('{{_globals._menu._boxmenu.graphicSrc}}'); webkit-background-size: cover; -moz-background-size: cover; -o-background-size: cover; background-size: cover; background-position: center center; background-repeat: no-repeat;">
			<div class="menu-header-inner">
				{{#if displayTitle}}
				<div class="menu-title">
					<div class="menu-title-inner h1 accessible-text-block" role="heading" aria-level="1" tabindex="0">
						{{{displayTitle}}}
					</div>
				</div>
				{{/if}}
				{{#if body}}
				<div class="menu-body">
					<div class="menu-body-inner accessible-text-block" aria-level="1" tabindex="0">
						{{{a11y_text body}}}
					</div>
				</div>
				{{/if}}
			</div>
		</div>

	</div>
	<div class="aria-label relative a11y-ignore-focus prevent-default" tabindex="0" role="region">{{_globals._menu._boxmenu.menuEnd}}</div>
	<div class="duration-bar-home">
	    <div class="menu-item-inner" style="left: 50%; margin-left: 0px;">
	        <a href="#" class="numspothome" role="button" aria-label=""><div class="menu-tooltip">Home</div><div class="homespot" aria-level="1" tabindex="0"></div></a>
		</div>
	</div>
</div>
